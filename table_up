    // 审核标识（0-基础版本or1-一级or2-二级or3-over）
    propsAuditFlag: {
      type: Number,
      default: 2,
    },
    // 是否可以编辑
    isEditFlag: {
      type: Boolean,
      default: false,
    },
    // 表格数据
    tableData: {
      type: Array,
      default() {
        return [
