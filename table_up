            },
            {
              width: '70px',
              name: 'unitPrice',
            },
            {
              width: '80px',
              name: 'subtotal',
            }
          )
          this.lableArray.splice(
            7,
            0,
            {
              label: '审核数量',
              width: '90px',
            },
            {
